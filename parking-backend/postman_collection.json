{
  "info": {
    "name": "Parking Management API",
    "description": "Complete API collection for parking management system",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000/api"
    },
    {
      "key": "token",
      "value": ""
    },
    {
      "key": "userId",
      "value": ""
    },
    {
      "key": "bookingId",
      "value": ""
    },
    {
      "key": "locationId",
      "value": ""
    },
    {
      "key": "parkingLotId",
      "value": ""
    }
  ],
  "item": [
    {
      "name": "Auth",
      "item": [
        {
          "name": "Register Customer",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "url": "{{baseUrl}}/auth/register",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"customer1\",\n  \"password\": \"password123\",\n  \"email\": \"customer@example.com\",\n  \"role\": \"customer\"\n}"
            }
          }
        },
        {
          "name": "Register Admin",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "url": "{{baseUrl}}/auth/register",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"admin1\",\n  \"password\": \"admin123\",\n  \"email\": \"admin@example.com\",\n  \"role\": \"admin\"\n}"
            }
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "url": "{{baseUrl}}/auth/login",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"customer1\",\n  \"password\": \"password123\"\n}"
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('token', response.token);",
                  "    pm.collectionVariables.set('userId', response.user.id);",
                  "}"
                ]
              }
            }
          ]
        }
      ]
    },
    {
      "name": "Admin",
      "item": [
        {
          "name": "Create Location",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/admin/locations",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Downtown Plaza\",\n  \"address\": {\n    \"street\": \"123 Main St\",\n    \"city\": \"New York\",\n    \"zipCode\": \"10001\"\n  },\n  \"coordinates\": {\n    \"latitude\": 40.7128,\n    \"longitude\": -74.0060\n  }\n}"
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('locationId', response._id);",
                  "}"
                ]
              }
            }
          ]
        },
        {
          "name": "Get Locations",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/admin/locations"
          }
        },
        {
          "name": "Create Parking Lot",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/admin/parking-lots",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Main Street Parking\",\n  \"location\": \"{{locationId}}\",\n  \"totalSpots\": 50,\n  \"ratePerHour\": 5.00,\n  \"amenities\": [\"security\", \"covered\"]\n}"
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('parkingLotId', response._id);",
                  "}"
                ]
              }
            }
          ]
        },
        {
          "name": "Get Parking Lots",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/admin/parking-lots"
          }
        },
        {
          "name": "Dashboard Stats",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/admin/dashboard"
          }
        }
      ]
    },
    {
      "name": "Bookings",
      "item": [
        {
          "name": "Search Parking",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": {
              "raw": "{{baseUrl}}/bookings/search?startTime=2024-12-01 09:00&endTime=2024-12-01 17:00&location=downtown",
              "host": ["{{baseUrl}}"],
              "path": ["bookings", "search"],
              "query": [
                {"key": "startTime", "value": "2024-12-01 09:00"},
                {"key": "endTime", "value": "2024-12-01 17:00"},
                {"key": "location", "value": "downtown"}
              ]
            }
          }
        },
        {
          "name": "Create Booking",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/bookings",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"parkingLot\": \"{{parkingLotId}}\",\n  \"startTime\": \"2024-12-01 09:00\",\n  \"endTime\": \"2024-12-01 17:00\",\n  \"vehicleDetails\": {\n    \"type\": \"car\",\n    \"licensePlate\": \"ABC123\"\n  }\n}"
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('bookingId', response._id);",
                  "}"
                ]
              }
            }
          ]
        },
        {
          "name": "Get My Bookings",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/bookings/my-bookings"
          }
        },
        {
          "name": "Cancel Booking",
          "request": {
            "method": "PUT",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/bookings/{{bookingId}}/cancel"
          }
        }
      ]
    },
    {
      "name": "Payments",
      "item": [
        {
          "name": "Create Payment Intent",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/payments/create-intent",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"bookingId\": \"{{bookingId}}\"\n}"
            }
          }
        },
        {
          "name": "Confirm Payment",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/payments/confirm",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"paymentIntentId\": \"pi_test_123\",\n  \"bookingId\": \"{{bookingId}}\"\n}"
            }
          }
        },
        {
          "name": "Payment History",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/payments/history"
          }
        }
      ]
    },
    {
      "name": "Profile",
      "item": [
        {
          "name": "Get Profile",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/profile/me"
          }
        },
        {
          "name": "Update Profile",
          "request": {
            "method": "PUT",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/profile/me",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"updated@example.com\",\n  \"phone\": \"+1234567890\"\n}"
            }
          }
        },
        {
          "name": "Add Vehicle",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/profile/vehicles",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"type\": \"car\",\n  \"licensePlate\": \"XYZ789\",\n  \"make\": \"Toyota\",\n  \"model\": \"Camry\",\n  \"year\": 2020\n}"
            }
          }
        }
      ]
    },
    {
      "name": "Reports",
      "item": [
        {
          "name": "Revenue Report",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": {
              "raw": "{{baseUrl}}/reports/revenue?startDate=2024-01-01&endDate=2024-12-31&groupBy=month",
              "host": ["{{baseUrl}}"],
              "path": ["reports", "revenue"],
              "query": [
                {"key": "startDate", "value": "2024-01-01"},
                {"key": "endDate", "value": "2024-12-31"},
                {"key": "groupBy", "value": "month"}
              ]
            }
          }
        },
        {
          "name": "Customer Report",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": {
              "raw": "{{baseUrl}}/reports/customers?startDate=2024-01-01&endDate=2024-12-31",
              "host": ["{{baseUrl}}"],
              "path": ["reports", "customers"],
              "query": [
                {"key": "startDate", "value": "2024-01-01"},
                {"key": "endDate", "value": "2024-12-31"}
              ]
            }
          }
        }
      ]
    }
  ]
}